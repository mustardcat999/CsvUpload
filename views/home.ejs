


    <h1 class="text-center">Uploaded Files</h1>

   
      <div class="table-responsive">
        <table class="table table-dark table-bordered table-striped mb-0">
            <thead>
                <tr>
                  <th>Filename</th>
                  <th>Actions</th>
                </tr>
              </thead>
              <tbody>
                <% for(data of files){ %>
                  <tr>
                    <td> <%=data.file%></td>
                    <td>
                      <a href="/csv/<%= data._id %>">View</a>
                    </td>
                  </tr>
               <%}%>
              </tbody>
     
     
        </table>
      </div>


    <div class="container mb-5">
        <div class="text-center">
                <form action="/upload" method="post" enctype="multipart/form-data">
                    <div class="form-group mt-5">
                        <label for="csvFile">Select a CSV file to upload:</label>
                        <input type="file" class="form-control-file" id="csv_file" name="csv_file" accept=".csv">
                    </div>
                    <button type="submit" class="btn btn-primary text-center mt-2">Upload</button>
                </form>
        </div>
    </div>